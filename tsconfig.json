{
    "compilerOptions": {
        "jsx": "react",
        "outDir": "./dist",
        "module": "commonjs",
        "target": "es5",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*"
    ]
}
